    private void volverActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_volverActionPerformed
        // TODO add your handling code here:
        if (evt.getSource()==volver){
            inicio_sesion retorna =new inicio_sesion();
            retorna.setVisible(true);
            this.setVisible(false);}
    }//GEN-LAST:event_volverActionPerformed

    private void modelo2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_modelo2ActionPerformed
        // TODO add your handling code here:
        if (evt.getSource()==modelo2){
            iniciomodelo2 ingreso=new iniciomodelo2();
            ingreso.setVisible(true);
            this.setVisible(false);
        } 
       
    }//GEN-LAST:event_modelo2ActionPerformed

