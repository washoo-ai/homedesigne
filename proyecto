    private void ruralMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_ruralMouseClicked
        if (evt.getSource()==rural){
            iniciomodelo1 ingreso=new iniciomodelo1();
            ingreso.setVisible(true);
            this.setVisible(false);
        }
        // TODO add your handling code here:
    }//GEN-LAST:event_ruralMouseClicked

