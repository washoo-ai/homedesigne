    private void atrasActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_atrasActionPerformed
        // TODO add your handling code here:
        if (evt.getSource()==atras){
            modoinicio retorna =new modoinicio();
            retorna.setVisible(true);
            this.setVisible(false);}
    }//GEN-LAST:event_atrasActionPerformed

    private void siguienteActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_siguienteActionPerformed
        // TODO add your handling code here:
        
        String usuario,contraseña;
        usuario=textousuario.getText();
        contraseña=textocontraseña.getText();
        
       
    
        
        if (usuario.equals("0302686134")&&contraseña.equals("0302686134")){
    inicio_arquitecto ingreso=new inicio_arquitecto();
    ingreso.setVisible(true);
    dispose();
    
    
    }
        else{ if (usuario.equals("cliente")&&contraseña.equals("0302686134")){
    inicio_cliente ingreso=new inicio_cliente();
    ingreso.setVisible(true);
    dispose();
    
    
    }
         else{  if (usuario.equals("presupuestario")&&contraseña.equals("0302686134")){
    inicio_presupuestario ingreso=new inicio_presupuestario();
    ingreso.setVisible(true);
    dispose();
    
    
    }
        
        
        
        else{
            JOptionPane.showMessageDialog(null, "usuario o contraseña invalidos");
    }
        }
        }
            
    
            
    
        
        
       
         
      
    }//GEN-LAST:event_siguienteActionPerformed

